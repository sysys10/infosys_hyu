{
  "arrowParens": "always",
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "tabWidth": 2,
  "printWidth": 180,
  "semi": false,
  "importOrder": ["^@apis$", "<THIRD_PARTY_MODULES>", "^@types$", "^@hooks$", "^@pages$", "^@styles$", "^@utils$", "^@packages/(.*)$", "^[./]"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
